package com.company;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        int []s= new int[4];
        Scanner cin = new Scanner(System.in);
        for(int i = 0; i < 4; i++)
        {
            s[i] = cin.nextInt();
        }
        System.out.println(saltosh(s));
    }
    static int saltosh(int[]mass) {
        int max = -9999;
        for( int i = 0; i < 4; i++)
        {
            if(max < mass[i])
                max = mass[i];
        }
        return max;
    }
}